{
	"name": "moneo-ts",
	"version": "1.6.0",
	"description": "monad library for typescript",
	"license": "MIT",
	"author": "patrickap",
	"repository": "https://github.com/patrickap/moneo-ts.git",
	"type": "module",
	"files": [
		"./dist"
	],
	"main": "./dist/moneo-ts.umd.cjs",
	"module": "./dist/moneo-ts.js",
	"exports": {
		".": {
			"import": "./dist/moneo-ts.js",
			"require": "./dist/moneo-ts.umd.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"types": "./dist/index",
	"scripts": {
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"build": "tsc && vite build",
		"release": "npm run test && npm run build && dotenv semantic-release",
		"release:dry": "dotenv semantic-release -- -d"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"dotenv-cli": "^8.0.0",
		"semantic-release": "^24.2.3",
		"typescript": "^5.8.3",
		"vite": "^6.2.6",
		"vite-plugin-dts": "^4.5.3",
		"vitest": "^3.1.1"
	},
	"keywords": [
		"monad",
		"adt",
		"functional",
		"fp",
		"typescript",
		"pure",
		"option",
		"either",
		"io",
		"async"
	]
}
